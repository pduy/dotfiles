URxvt.intensityStyles:	false
URxvt.depth:		32
!URxvt.font: 		xft:Source Code Pro:pixelsize=12
!URxvt.font: 		xft:DejaVu Sans Mono for Powerline:size=11
URxvt.font: 		xft:Inconsolata for Powerline:size=11
URxvt.lineSpace:	3
URxvt.scrollBar:	false
URxvt.cursorColor:	white
URxvt.cusorBlink:	false
URxvt.saveLines:	200000
Xcursor.theme:		DMZ-White
Xcursor.size:		24
!URxvt*transparent: true
!URxvt*shading: 10

!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select,clipboard
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
!URxvt.url-select.launcher: linkhandler
URxvt.url-select.launcher: chromium
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
!URxvt.keysym.M-s: perl:keyboard-select:search
!! resize-font
URxvt.resize-font.smaller: C-o
URxvt.resize-font.bigger: C-i
URxvt.resize-font.reset: C-equal
!! Matcher
!URxvt.url-launcher: linkhandler
URxvt.url-launcher: qutebrowser
URxvt.matcher.button: 1
!!Toggle solarized
!URxvt.keysym.M-c:	perl:solarized:toggle
!URxvt.solarized.default: dark

rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency:	true
rofi.lines:		5
rofi.bw:		0
rofi.opacity:		"20"
rofi.hide-scrollbar:	true
rofi.width:		50

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False

URxvt.clipboard.autocopy: true

Xft.dpi: 110

! These might also be useful depending on your monitor and personal preference:
!Xft.autohint: 0
!Xft.lcdfilter:  lcddefault
!Xft.hintstyle:  hintfull
!Xft.hinting: 1
!Xft.antialias: 1
!Xft.rgba: rgb
